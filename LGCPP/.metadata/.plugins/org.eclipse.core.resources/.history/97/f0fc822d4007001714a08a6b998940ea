#include "ShapeManager.h"

ShapeManager::ShapeManager() {
	numShapes = 0;
}

void ShapeManager::add(Shape *s) {
	shapes[numShapes] = s;
	numShapes ++;
}

ostream& operator<<(ostream &os, ShapeManager &sm) {
	for (int i = 0; i < sm.numShapes; i++) {
		Shape* &temp = sm.shapes[i];

	}
	return os;
}

ShapeManager::~ShapeManager() {

}
