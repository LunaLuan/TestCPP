#include "Rectangle.h"

Rectangle::Rectangle(int a, int b) {
	m_a = a;
	m_b = b;
}

int Rectangle::getArea() {
	return m_a * m_b;
}

int Rectangle::getPerimeter() {
	return edges[0] * 2 + edges[1] * 2;
}

int Rectangle::getNumEdges() {
	return numEdges;
}

Rectangle::~Rectangle() {
	delete[] edges;
}
